<h2>New Book</h2>
<%= form_with model: book do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title,class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :body %>
    <%= f.text_area :body,class: "form-control" %>
  </div>
  <%=f.submit "Create Book",class: "btn btn-success" %>
<% end %>